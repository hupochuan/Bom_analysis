#Unigram
U0:%x[-2,0]
U01:%x[-1,0]
U02:%x[0,0]
U03:%x[1,0]
U04:%x[2,0]
U05:%x[-2,1]
U06:%x[-1,1]
U07:%x[0,1]
U08:%x[1,1]
U09:%x[2,1]
U10:%x[0,0]/%x[1,0]
U11:%x[-1,0]/%x[0,0]
U11:%x[-2,0]%x[-1,0]
U12:%x[1,0]%x[2,0]
U13:%x[1,0]%x[2,0]%x[3,0]
U14:%x[-2,0]%x[-1,0]%x[0,0]
U15:%x[-1,0]%x[0,0]%x[1,0]
U16:%x[0,0]%x[1,0]%x[2,0]
U17:%x[-2,1]%x[-1,1]
U18:%x[-1,1]%x[0,1]
U19:%x[0,1]%x[1,1]
U20:%x[1,1]%x[2,1]
U21:%x[-2,1]%x[-1,1]%x[0,1]
U22:%x[-1,1]%x[0,1]%x[1,1]
U23:%x[0,1]%x[1,1]%x[2,1]
